<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">EasyAccordion</string>
    <declare-styleable name="accordion">

        <attr format="boolean" name="isPartitioned"/>
        <attr format="boolean" name="isAnimated"/>
        <attr format="string" name="heading"/>
        <attr format="dimension" name="headingTextSize"/>
        <attr format="boolean" name="isExpanded"/>
        <attr format="color" name="headingBackgroundColor"/>
        <attr format="color" name="bodyBackgroundColor"/>
        <attr format="reference" name="headingBackground"/>
        <attr format="reference" name="bodyBackground"/>


    </declare-styleable>
</resources>